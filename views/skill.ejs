<%- include('./partials/header.ejs')%>
<div class="container-fluid bg-dark">
    <div class="row">
        <div class="col-sm-2">
            <%- include('./partials/sidebar.ejs', {pageName:'Skill'}) %>
        </div>
        <div class="col-sm-10">
            <!--Main content-->
            <div class="container mt-5">
                <h1 class="border-bottom text-warning mb-5">SKILLS</h1>

                <div class="text-white row">
                    <div class="col-sm-6">
                        <h3 class="mt-5">HTML/CSS</h3>
                        <div class="progress">
                            <div class="progress-bar bg-danger" role="progressbar" style="width: 100%;" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100">100%</div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <h3 class="mt-5">JavaScript</h3>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" style="width: 85%;" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100">85%</div>
                        </div>
                    </div>
                </div>

                <div class="text-white row">
                    <div class="col-sm-6">
                        <h3 class="mt-5">Node.js</h3>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated bg-success" role="progressbar" style="width: 80%;" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100">80%</div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <h3 class="mt-5">Express</h3>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated bg-info" role="progressbar" style="width: 90%;" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100">90%</div>
                        </div>
                    </div>
                </div>

                <div class="text-white row">
                    <div class="col-sm-6">
                        <h3 class="mt-5">MongoDB</h3>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated bg-warning" role="progressbar" style="width: 60%;" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100">60%</div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <h3 class="mt-5">React</h3>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated bg-primary" role="progressbar" style="width: 90%;" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100">90%</div>
                        </div>
                    </div>
                </div>

                <div class="text-white row">
                    <div class="col-sm-6">
                        <h3 class="mt-5">Java</h3>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated bg-secondary" role="progressbar" style="width: 85%;" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100">85%</div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <h3 class="mt-5">C++</h3>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated bg-info" role="progressbar" style="width: 70%;" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100">70%</div>
                        </div>
                    </div>
                </div>

                <div class="text-white row">
                    <div class="col-sm-6">
                        <h3 class="mt-5">Python</h3>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated bg-success" role="progressbar" style="width: 95%;" aria-valuenow="95" aria-valuemin="0" aria-valuemax="100">95%</div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

<%- include('./partials/footer.ejs')%>
